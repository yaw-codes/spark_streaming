{
    "schemaName": "transaction_facts",
    "dimensionFieldSpecs": [
        { "name": "transaction_id", "dataType": "STRING" },
        { "name": "account_id", "dataType": "STRING" },
        { "name": "customer_id", "dataType": "STRING" },
        { "name": "transaction_type", "dataType": "STRING" },
        { "name": "currency", "dataType": "STRING" },
        { "name": "branch_id", "dataType": "STRING" }
    ],
    "metricFieldSpecs": [
        { "name": "transaction_amount", "dataType": "DOUBLE" },
        { "name": "exchange_rate", "dataType": "FLOAT" }
    ],
    "dateTimeFieldSpecs": [
        {
            "name": "transaction_date",
            "dataType": "LONG",
            "format": "1:MILLISECONDS:EPOCH",
            "granularity": "1:MILLISECONDS"
        }
    ]
}
